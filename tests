[901]
NAME="BASE-01"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  ls /usr
  echo Hello
  setenv aa
  echo hello
  ls -l /usr
  /usr/bin/ls /
  echo      aa      ss  bb
[901-END]

[902]
NAME="BASE-02 quotes"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo '
    echo hello world
    echo "hello"world
    echo " hello " "sodl dsd"
    echo "'aa'bb"
    echo "aa"'bb'
    echo "hello"aaa"hello"
    echo"hello world"
    echo "foo
[902-END]

