[904]
NAME="BASE redirect"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo foo >
    echo foo >>
    echo foo <
    echo foo <<
    echo "b" >< log
    a<b
    a>exe/noright
[904-END]

[905]
NAME="BASE pointvirgule"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ;;;
    ;
    echo hello; echo world
    echo hello;echo world
    echo hello ;echo world
[905-END]

