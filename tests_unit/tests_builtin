[001]
NAME="WHERE 01"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where
[001-END]

[002]
NAME="WHERE 02"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where wc
[002-END]

[003]
NAME="WHERE 03"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where ls cd wc echo
[003-END]

[004]
NAME="WHERE 04"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where cd
[004-END]

[005]
NAME="WHERE 05"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  where /bin/ls
[005-END]

[006]
NAME="REPEAT 01"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat 3 ls
[006-END]

[007]
NAME="REPEAT 02"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat 0 ls
[007-END]

[008]
NAME="REPEAT 03"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat 0 ls > mdr ; rm mdr
[008-END]

[009]
NAME="REPEAT 04"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat -1 ls
[009-END]

[010]
NAME="REPEAT 05"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat 2 ls ; repeat 2 echo e
[010-END]

[011]
NAME="REPEAT 06"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  repeat 2 ls | grep t
[011-END]

[012]
NAME="Precmd"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias precmd echo pre
  ls
  unalias precmd
  ls
[012-END]

[013]
NAME="Cwdcmd"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias cwdcmd echo cd
  cd ..
  unalias cwdcmd
  cd ..
[013-END]

[014]
NAME="Cwdcmd + Precmd"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias precmd echo pre
  alias cwdcmd echo cd
  cd ..
  ls
[014-END]

[015]
NAME="Alias"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias echo ls
  echo
  ls
[015-END]

[016]
NAME="Alias small loop"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias echo ls
  alias ls echo
[016-END]

[017]
NAME="Alias big loop"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias echo ls
  alias ls cd
  alias cd echo
[017-END]

[018]
NAME="Unalias"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  alias test ls
  test
  unalias test
  test
[018-END]

[019]
NAME="Set and unset"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  set test=alpha
  echo $test
  unset test
  echo $test
[019-END]
