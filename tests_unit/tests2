[001]
NAME="LINE FORMATING"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  ls     -l     |    cat
[001-END]

[002]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ls || echo && e || ls
    xx &&echo X && echo A || echo E
    xx || echo V && echo O || echo P
[002-END]

[003]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    e && echo X && echo J
    e || ls && echo E
[003-END]

[004]
NAME="LINE FORMATING"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    \ls
[004-END]

[005]
NAME="Backquotes"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo `ls && echo || ls && ls || ls ||Â echo`
[005-END]

[006]
NAME="Inhibitor"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo \" hello world \'
[006-END]

[007]
NAME="Inhibitor"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo \| hello world\ \ \*
[007-END]

[008]
NAME="Inhibitor"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo \\
[008-END]

[009]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo e && echo e && echo e
[009-END]

[010]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo e || echo e && echo e
[010-END]

[011]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    xxx && echo e && echo e
[011-END]

[012]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo e && echo e && xxx
[012-END]

[013]
NAME="&& ||"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo e && xxx && echo e
[013-END]

[014]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ls (ls) > log
[014-END]

[015]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    (ls) > log ; cat -e log
[015-END]

[016]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    (echo A | cat -e ; (ls) > log; echo e) > log
[016-END]

[017]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    (echo A | cat -e && ls && (ls -l; echo A); echo $PWD)
[017-END]

[018]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ( ls ( ls ) )
[018-END]

[019]
NAME="parenthesis"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ( ls ) ( ls )
[019-END]

[020]
NAME="X"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
                            x
[020-END]

[021]
NAME="parenthesis + &&"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    (ls) && echo e
[021-END]

[022]
NAME="&& + X"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ls && echo e && echo a && ls && xx
[022-END]

[023]
NAME="Which"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    alias ls cd
    which ls
[023-END]

[024]
NAME="boolop"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ls -l /dev && echo displayOK || echo err_disp4
    ls -l /azerty && echo not_display || echo err_disp5
[024-END]

[025]
NAME="VAR"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
        set hello=world; echo x$helloY; echo x${hello}Y
[025-END]

