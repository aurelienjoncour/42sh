[5-END]

[6]
NAME="BASE 6 - fail redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo foo >
    echo foo >>
    echo foo <
    echo foo <<
    echo "b" >< log
    a<b
    a>exe/noright
[6]

[7]
NAME="BASE 7 - semicolon"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    ;;;
    ;
    echo hello; echo world
    echo hello;echo world
    echo hello ;echo world
[7-END]

[8]
NAME="BASE 8 - ambiguous redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    echo "Hello world" > log >> log
[8-END]

