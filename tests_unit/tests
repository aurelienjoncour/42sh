[28]
NAME="BASE 28 - double redirections"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
    cat < Makefile > log
    cat << HEL > log
    cat > log << HEL
    >> log cat << HEL
[28-END]

